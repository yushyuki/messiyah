<p id="notice"><%= notice %></p>

<p>
  <strong>Picture:</strong>
  <%= profile_image_for @post %>
  <% if !@post.image2.nil? %>
  <%= profile_image2_for @post %>
  <% end %>
  <% if !@post.image3.nil? %>
  <%= profile_image3_for @post %>
  <% end %>
  <% if !@post.image4.nil? %>
  <%= profile_image4_for @post %>
  <% end %>
  <% if !@post.image5.nil? %>
  <%= profile_image5_for @post %>
  <% end %>
</p>

<p>
  <strong>Content:</strong>
  <%= @post.content %>
</p>

<p>
  <strong>User:</strong>
  <%= @post.user_id %>
</p>

<%= link_to 'Edit', edit_post_path(@post) %> |
<%= link_to 'Back', posts_path %>
